    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Tests",
            "request": "launch",
            "mainClass": "com.eudycontreras.othello.unittest.Tests",
            "projectName": "Othello FX"
        },
        {
            "type": "java",
            "name": "Tester",
            "request": "launch",
            "mainClass": "com.eudycontreras.othello.views.Tester",
            "projectName": "Othello FX"
        },
        {
            "type": "java",
            "name": "AgentManager",
            "request": "launch",
            "vmArgs": "--module-path \"/Users/z.alfredji/Documents/javafx-sdk-21.0.1/lib\" --add-modules javafx.controls,javafx.media,javafx.web,javafx.graphics,javafx.fxml",
            "mainClass": "main.AgentManager",
            "projectName": "Othello FX"
        }
    ]
}